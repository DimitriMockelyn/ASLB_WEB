{
  "author": "dimitri.mockelyn@gmail.com",
  "name": "app",
  "description": "Site ASLB",
  "version": "1.0.0",
  "engines": {
    "node": "0.4.x",
    "npm": "2.14.x"
  },
  "scripts": {
    "bundle": "better-npm-run bundle",
    "build": "npm run bundle",
    "watch": "better-npm-run watch"
  },
  "main": "app/index.js",
  "babel": {
    "presets": [
      "focus"
    ]
  },
  "betterScripts": {
    "bundle": {
      "command": "webpack --progress",
      "env": {
        "ANCHOR_CLASS": "app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "DEV": false,
        "NODE_ENV": "production",
        "LIBRARY_NAME": "ASLB",
        "OUTPUT_DIR": "./dist",
        "PACKAGE_JSON_PATH": "./"
      }
    },
    "watch": {
      "command": "webpack --progress --watch",
      "env": {
        "ANCHOR_CLASS": "app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "DEV": false,
        "NODE_ENV": "production",
        "LIBRARY_NAME": "ASLB",
        "OUTPUT_DIR": "./dist",
        "PACKAGE_JSON_PATH": "./",
        "GENERATE_HTML": false,
        "SOURCE_MAPS": false,
        "BABEL_ENV": "production"
      }
    },
    "deliver": {
      "command": "webpack --progress",
      "env": {
        "ANCHOR_CLASS": "app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "DEV": false,
        "NODE_ENV": "production",
        "LIBRARY_NAME": "ASLB",
        "OUTPUT_DIR": "./dist",
        "PACKAGE_JSON_PATH": "./",
        "GENERATE_HTML": false,
        "SOURCE_MAPS": false,
        "MINIMIFY": true
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.3.3",
    "d3": "3.5.14",
    "focus-components": "2.1.1",
    "focus-core": "2.1.1",
    "focus-devtools": "^0.3.5",
    "focus-notifications": "^0.2.0-rc6",
    "google-auth-library": "^0.11.0",
    "googleapis": "^22.2.0",
    "has-flag": "^2.0.0",
    "immutable": "^3.7.2",
    "input-moment": "^0.3.6",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "3.0.0-alpha1",
    "lodash": "^3.10.1",
    "material-design-icons": "^2.0.0",
    "moment": "2.10.3",
    "native-promise-only": "^0.8.0-a",
    "numeral": "^1.5.3",
    "react": "^15.2.1",
    "react-big-calendar": "^0.15.0",
    "react-datetime": "^2.10.3",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "react-rte-image": "^0.11.1",
    "react-star-rating-component": "^1.3.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "animate.css": "^3.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "6.5.0",
    "babel-preset-focus": "^0.5.2",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.5",
    "body-parser": "^1.14.1",
    "css-loader": "^0.21.0",
    "electron-prebuilt": "0.36.9",
    "eslint": "1.5.1",
    "eslint-config-focus": "0.3.0",
    "eslint-plugin-filenames": "0.1.1",
    "eslint-plugin-react": "3.5.0",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "focus-file": "^0.7.3",
    "font-awesome": "^4.5.0",
    "json-loader": "^0.5.3",
    "material-design-icons-iconfont": "^2.0.4",
    "migration-tool": "^0.1.0",
    "react-addons-css-transition-group": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-focus": "^0.11.5"
  }
}
